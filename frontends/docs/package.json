    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "next": "^13.0.0",
    "react": "^18.2.0",
    "clsx": "^1.1.1",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0",
    "ui-component": "*"
  },
  "devDependencies": {
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "prisma": "^4.7.1",
    "npm-run-all": "^4.1.5",
    "@tooling/eslint-config-custom": "*",
    "@tooling/jest": "*",
    "tailwind-config": "*",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3"
