{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "db:generate"],
      "outputs": ["dist/**", ".next/**"],
      "env": ["VERCEL_URL", "PORT", "NODE_ENV"]
    },
    "db:generate": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["db:generate"],
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "precommit:format": {
      "cache": false
    }
  }
}
